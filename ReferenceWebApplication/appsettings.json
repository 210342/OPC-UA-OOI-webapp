{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "M2MCommunication":  "Warning"
    }
  },
  "AllowedHosts": "*",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "UaLibrary": {
    "ConsumerConfigurationFile": "ConfigurationDataConsumer.BoilersSet.xml",
    "LibraryDirectory": "M2MCommunication.Uaooi",
    "ResourcesDirectory": "wwwroot"
  },

  "ImageRepository": {
    "Directory": "wwwroot\\Images",
    "PropertiesFileName": "Properties.json"
  }
}
