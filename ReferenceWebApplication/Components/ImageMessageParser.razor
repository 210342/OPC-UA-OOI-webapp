@using MessageParsing

<h3>ImageMessageParser</h3>

<div class="image-container">
    @foreach (DrawableProperty property in Properties.OfType<DrawableProperty>())
    {
        <Property @key="property" DrawableProperty="property"/>
    }
</div>

@code {
    [Parameter]
    public IEnumerable<IProperty> Properties { get; set; }
}
